            -- Datos del paciente
            p.pac_nombres,
            p.pac_apellidos,
            p.pac_cedula,
            p.pac_his_cli,

            -- Datos de la atenci贸n
            a.ate_fecha,
            a.ate_hora,

            -- Datos del profesional que complet贸
            u.usu_nombre,
            u.usu_apellido,
            r.rol_nombre,

            -- Datos del admin que habilit贸 (si aplica)
            admin.usu_nombre as admin_nombre,
            admin.usu_apellido as admin_apellido,

            -- CORREGIDO: Determinar si puede modificar - LGICA COMPLETA
            CASE
                WHEN fu.modificaciones_usadas >= fu.modificaciones_permitidas THEN 0  -- NO puede si ya alcanz贸 el l铆mite
                WHEN fu.habilitado_por_admin = 1 THEN 1  -- S puede si est谩 habilitado por admin
                WHEN fu.modificaciones_usadas < fu.modificaciones_permitidas THEN 1  -- S puede si tiene modificaciones restantes
                ELSE 0
            END as puede_modificar,

            -- CORREGIDO: Motivo del bloqueo si aplica
            CASE
                WHEN fu.modificaciones_usadas >= fu.modificaciones_permitidas THEN 'L铆mite de modificaciones alcanzado (3/3)'
                WHEN fu.habilitado_por_admin = 1 THEN 'Habilitado por administrador'
                WHEN fu.modificaciones_usadas < fu.modificaciones_permitidas THEN 'Puede modificar naturalmente'
                ELSE 'Sin modificaciones disponibles'
            END as motivo_bloqueo,

            -- NUEVO: Estado para el bot贸n del admin
