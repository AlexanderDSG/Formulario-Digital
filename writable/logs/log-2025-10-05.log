        JOIN t_paciente p ON a.pac_codigo = p.pac_codigo
        JOIN t_usuario u ON fu.usu_id = u.usu_id
        JOIN t_rol r ON u.rol_id = r.rol_id
        LEFT JOIN t_usuario admin ON fu.admin_que_habilito = admin.usu_id

