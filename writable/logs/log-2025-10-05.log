            -- Datos del paciente
            p.pac_nombres,
            p.pac_apellidos,
            p.pac_cedula,
            p.pac_his_cli,

            -- Datos de la atenci贸n
            a.ate_fecha,
            a.ate_hora,

            -- Datos del profesional que complet贸
