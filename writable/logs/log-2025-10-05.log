            CASE
                WHEN fu.seccion = 'ME' THEN 'M茅dico de Triaje'
                WHEN fu.seccion = 'ES' THEN 'M茅dico Especialista'
                ELSE 'Otro'
            END as tipo_profesional

        FROM t_formulario_usuario fu
